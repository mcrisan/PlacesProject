            <div class="tab-content" style="padding:0px; border:0px">
         {% if app.session.get('aux')=='true' %}
                <div id="details" class="tab-pane">
                {% include 'BundlePlacesBundle:Places/Tabs:presentation.html.twig' with {'place':place.placeDetails, 'placePhotos' : place.firstPhoto } %}
                </div><!-- /.panel1 -->
         {%else%}
                <div id="details" class="tab-pane active">
                {% include 'BundlePlacesBundle:Places/Tabs:presentation.html.twig' with {'place':place.placeDetails, 'placePhotos' : place.firstPhoto } %}
                </div><!-- /.panel1 -->
         {%endif%}
                <!-- Photos section -->
                <div id="photos" class="tab-pane">
                </div><!-- /.pane2 -->

                <!-- Vote section -->
                <div id="ratings" class="tab-pane">
                </div><!-- /.pane3 -->

                <div id="reviews" class="tab-pane">
                </div><!-- /.panel4 -->

                <div id="promotions" class="tab-pane">
                </div><!-- /.panel5 -->
         {% if app.session.get('aux')=='true' %}
                <div id="events" class="tab-pane active">
                     {% include 'BundlePlacesBundle:Places/Tabs:events.html.twig' with { 'place':place, 'isowner' : isowner } %}
                </div><!-- /.panel6 -->

         {% else %}
                <div id="events" class="tab-pane">
                     {% include 'BundlePlacesBundle:Places/Tabs:events.html.twig' with { 'place':place, 'isowner' : isowner } %}
                </div><!-- /.panel6 -->
         {% endif %}
            </div><!-- /.tab-content -->
        </div><!-- /.tabbable -->
    </section>

    <section class="map">
        <div class="header">
            <div class="address" ng-controller="TypeaheadCtrl">
                <div class="marker"></div>
                <label>Dorobantilor 48, CLUJ NAPOCA</label>
                <input type="hidden" id="fromAddress" name="from" value="Calea Dorobantilor 48, Cluj Napoca, Romania" placeholder="location address"/>
                <input ng-model="asyncSelected" value="" typeahead = "address for address in getLocation($viewValue) | filter:$viewValue" typeahead-loading="loadingLocations"></input>
            </div>
            <button class="location-button">
                <label>Change Location</label>
                <div class="pen-icon"></div>
            </button>
        </div>
        <div id="googleMap"></div>
    </section>
</div>
<form data-toggle="validator" role="form" id="formid">
<div class="modal fade" id="formOwner" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title blockquote-reverse" id="myModalLabel">Ownership</h4>
            </div>
            <div class="modal-body">
                <ul class="nav nav-list">
                    
                    <input type="hidden" id="placeid" name="placeid" value="{{ place.placeId }}"/>
                    
                    <div class="form-group">
                        <label for="inputName" class="control-label">Name</label>
                        <input type="text" class="form-control" id="inputName" placeholder="Enter Name" data-error="Enter your name." required>
                        <div class="help-block with-errors"></div>
                     </div>
                    
                     <div class="form-group">
                        <label for="inputEmail" class="control-label">Email</label>
                        <input type="email" class="form-control" id="inputEmail" placeholder="Email" data-error="This email is not valid." required>
                            <div class="help-block with-errors"></div>
                     </div>
                   <div class="form-group">
                        <label class="control-label" for="firstname">Tel:</label>
                     <div class="input-group">
                        <span class="input-group-addon">+4</span>
                        <input id='inputTel' class="form-control" placeholder="Enter Number" type="number" min="100000000" max="999999999" data-error="This number is not valid. Type like model: 0751234567" required />
                      </div>
                                <div class="help-block with-errors"></div>
                   </div>
                    <blockquote class="blockquote-reverse">
                        <p>Aceste date sunt private.</p>
                        <p>Va vom contacta pentru a va acorda drepturi de administrare a Place-ului.</p>
                        <p>Va multumim!</p>
                    </blockquote>
                </ul>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button id='send_owner_details' type="submit" disabled="disabled" class="btn btn-primary">Send</button>
            </div>
        </div>
    </div>
</div>
</form>